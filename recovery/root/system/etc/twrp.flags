# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<mnt_point>        <type>    <device>                                                              <mnt_flags and options>
/system_root        ext4      /dev/block/bootdevice/by-name/system                     flags=display="System Root";logical;slotselect;backup=0;wipeingui
/system             ext4      /dev/block/bootdevice/by-name/system                     flags=display="System";logical;slotselect;backup=0;wipeingui
/product            ext4      /dev/block/bootdevice/by-name/product                    flags=display="Product";logical;slotselect;backup=0;wipeingui
/vendor             ext4      /dev/block/bootdevice/by-name/vendor                     flags=display="Vendor";logical;slotselect;backup=0;wipeingui

/vendor_image       emmc      /dev/block/bootdevice/by-name/vendor                     flags=display="Vendor Image";backup=1;flashimg=1
/product_image      emmc      /dev/block/bootdevice/by-name/product                    flags=display="Product Image";backup=1;flashimg=1
/system_image       emmc      /dev/block/bootdevice/by-name/system                     flags=display="System Image";backup=1;flashimg=1

/super_image        emmc      /dev/block/platform/bootdevice/by-name/super             flags=display="Super Image";backup=0;flashimg=1

/persist_image      emmc      /dev/block/platform/bootdevice/by-name/persist           flags=display="Persist Image";backup=1;flashimg=1
/cust_image		      emmc		  /dev/block/platform/bootdevice/by-name/cust	             flags=display="Cust Image";backup=1;flashimg=1

/metadata           ext4      /dev/block/platform/bootdevice/by-name/metadata          flags=display="Metadata";backup=1
/data               f2fs      /dev/block/platform/bootdevice/by-name/userdata          flags=fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized,keydirectory=/metadata/vold/metadata_encryption
/boot               emmc      /dev/block/platform/bootdevice/by-name/boot              flags=display="Boot";backup=1;flashimg=1;slotselect
/cache		          ext4		  /dev/block/platform/bootdevice/by-name/rescue		         flags=display="Cache"
/dtbo               emmc      /dev/block/platform/bootdevice/by-name/dtbo              flags=display="DTBO";backup=1

/storage		        f2fs		  /data/media/0						                                 flags=display="Internal Storage";backup=1;usermrf;fsflags="bind";removable

/vbmeta             emmc      /dev/block/platform/bootdevice/by-name/vbmeta            flags=display="VBMeta";backup=1;slotselect
/vbmeta_vendor      emmc      /dev/block/platform/bootdevice/by-name/vbmeta_vendor     flags=display="VBMeta Vendor";backup=1;slotselect
/vbmeta_system      emmc      /dev/block/platform/bootdevice/by-name/vbmeta_system     flags=display="VBMeta System";backup=1;slotselect

/boot_para          emmc      /dev/block/platform/bootdevice/by-name/boot_para         flags=display="Boot Para"
/logo               emmc      /dev/block/platform/bootdevice/by-name/logo              flags=display="Logo";backup=1;slotselect
/logo2              emmc      /dev/block/platform/bootdevice/by-name/logo2	           flags=display="Logo2";backup=1;slotselect
/misc               emmc      /dev/block/platform/bootdevice/by-name/misc              flags=display="Misc";backup=1;slotselect
/para               emmc      /dev/block/platform/bootdevice/by-name/para              flags=display="Para"
/persist            ext4      /dev/block/platform/bootdevice/by-name/persist           flags=display="Persist"

/protect_f          ext4      /dev/block/platform/bootdevice/by-name/protect1         flags=display="Protect_f"
/protect_s          ext4      /dev/block/platform/bootdevice/by-name/protect2          flags=display="Protect_s"
/nvdata             ext4      /dev/block/platform/bootdevice/by-name/nvdata            flags=display="Nvdata"
/nvcfg              ext4      /dev/block/platform/bootdevice/by-name/nvcfg             flags=display="Nvcfg"
/otp                emmc      /dev/block/platform/bootdevice/by-name/otp               flags=display="Otp"
/persistent         emmc      /dev/block/platform/bootdevice/by-name/frp               flags=display="Persistent"
/nvram              emmc      /dev/block/platform/bootdevice/by-name/nvram             flags=display="Nvram"
/proinfo            emmc      /dev/block/platform/bootdevice/by-name/proinfo           flags=display="Proinfo"

/bootloader         emmc      /dev/block/platform/bootdevice/by-name/lk                flags=display="Bootloader"
/bootloader2        emmc      /dev/block/platform/bootdevice/by-name/lk2               flags=display="Bootloader2"
/gz1                emmc      /dev/block/platform/bootdevice/by-name/gz1               flags=display="Gz1"
/gz2                emmc      /dev/block/platform/bootdevice/by-name/gz2               flags=display="Gz2"
/expdb              emmc      /dev/block/platform/bootdevice/by-name/expdb             flags=display="Expdb"
/seccfg             emmc      /dev/block/platform/bootdevice/by-name/seccfg            flags=display="Seccfg"
/tee1               emmc      /dev/block/platform/bootdevice/by-name/tee1              flags=display="Tee1"
/tee2               emmc      /dev/block/platform/bootdevice/by-name/tee2              flags=display="Tee2"
/mcupm_1            emmc      /dev/block/platform/bootdevice/by-name/mcupm_1           flags=display="Mcupm_1"
/mcupm_2            emmc      /dev/block/platform/bootdevice/by-name/mcupm_2           flags=display="Mcupm_2"
/efuse              emmc      /dev/block/platform/bootdevice/by-name/efuse             flags=display="Efuse"
/scp1               emmc      /dev/block/platform/bootdevice/by-name/scp1              flags=display="Scp1"
/scp2               emmc      /dev/block/platform/bootdevice/by-name/scp2              flags=display="Scp2"
/sspm_1             emmc      /dev/block/platform/bootdevice/by-name/sspm_1            flags=display="Sspm_1"
/sspm_2             emmc      /dev/block/platform/bootdevice/by-name/sspm_2            flags=display="Sspm_1"
/dpm_1              emmc      /dev/block/platform/bootdevice/by-name/dpm_1             flags=display="Dpm_1"
/dpm_2              emmc      /dev/block/platform/bootdevice/by-name/dpm_2             flags=display="Dpm_2"
/md1img             emmc      /dev/block/platform/bootdevice/by-name/md1img            flags=display="Md1img"
/md1dsp             emmc      /dev/block/platform/bootdevice/by-name/md1dsp            flags=display="Md1dsp"
/md1arm7            emmc      /dev/block/platform/bootdevice/by-name/md1arm7           flags=display="Md1arm7"
/md3img             emmc      /dev/block/platform/bootdevice/by-name/md3img            flags=display="Md3img"
/spmfw              emmc      /dev/block/platform/bootdevice/by-name/spmfw             flags=display="Mpmfw"
/pi_img             emmc      /dev/block/platform/bootdevice/by-name/pi_img            flags=display="Pi_img"

# External Storage
/sdcard1		        auto		  /dev/block/mmcblk0p1  	   /dev/block/mmcblk0	           flags=display="MicroSD Card";storage;wipeingui;removable
/usb_otg		        auto		  /dev/block/sda1		         /dev/block/sdd			           flags=display="USB-Storage";storage;wipeingui;removable
